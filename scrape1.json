   "name": "Elixir Bloody Mary",
   "ingredients": [
    {
     "name": "Old Bay Seasoning"
    },
    {
     "name": "Square One Organic vodka",
     "units": "2 oz"
    },
    {
     "name": "Dried dill",
     "units": "1 tsp"
    },
    {
     "name": "Ground white pepper",
     "units": "1 tsp"
    },
    {
     "name": "Celery salt",
     "units": "1 tsp"
    },
    {
     "name": "Worcestershire sauce",
     "units": "2 dashes"
    },
    {
     "name": "Tabasco Sauce",
     "units": "2 dashes"
    },
    {
     "name": "Elixir Juice (a mixture of olive brine, dill pickle brine and various other pickle brines)",
     "units": "1⁄2 oz"
    },
    {
     "name": "Lemon wedge's juice",
     "units": "1"
    },
    {
     "name": "Prepared horseradish",
     "units": "1 tsp"
    },
    {
     "name": "Tomato juice",
     "units": "4 oz"
    }
   ],
   "garnish": [
    {
     "name": "Kosher dill pickle"
    },
    {
     "name": "Cooked thick-cut bacon slice"
    }
   ],
   "howto": [
    {
     "text": "Coat the rim of a pint glass with Old Bay Seasoning."
    },
    {
     "text": "Add the remaining ingredients to a shaker and fill with ice."
    },
    {
     "text": "Shake lightly�-you want to mix the ingredients without melting too much of the ice�-and pour (unstrained) into the prepared glass."
    },
    {
     "text": "Garnish with a kosher dill pickle spear and a cooked slice of thick-cut bacon."
    },
    {
     "text": "Don't use a straw; drink from the rim in order to get a little of the Old Bay with each sip."
    }
